<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".cadastrovagaActivity">

    <Button
        android:id="@+id/button3"
        style="@style/Widget.Material3.Button.IconButton.Filled"
        android:layout_width="322dp"
        android:layout_height="58dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="#F44336"
        android:drawableTint="#2196F3"
        android:fontFamily="sans-serif-black"
        android:foregroundTint="#2196F3"
        android:shadowColor="#2196F3"
        android:text="Cadastro"
        android:textColor="#000000"
        android:textColorHighlight="#2196F3"
        android:textColorHint="#2196F3"
        android:textColorLink="#2196F3"
        android:textSize="18sp"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.252"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edSalario"
        app:rippleColor="#FFFFFF"
        app:strokeColor="#000000" />

    <EditText
        android:id="@+id/edEmpresa"
        android:layout_width="374dp"
        android:layout_height="61dp"
        android:layout_marginTop="172dp"
        android:ems="10"
        android:inputType="text"
        android:text="Empresa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edCargo"
        android:layout_width="374dp"
        android:layout_height="61dp"
        android:layout_marginTop="248dp"
        android:ems="10"
        android:inputType="text"
        android:text="Cargo"
        app:layout_constraintEnd_toEndOf="@+id/edEmpresa"
        app:layout_constraintHorizontal_bias="0.311"
        app:layout_constraintStart_toStartOf="@+id/edEmpresa"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edSalario"
        android:layout_width="374dp"
        android:layout_height="61dp"
        android:layout_marginTop="328dp"
        android:ems="10"
        android:inputType="text"
        android:text="Salario"
        app:layout_constraintEnd_toEndOf="@+id/edCargo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edCargo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="124dp"
        android:layout_height="69dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="sans-serif-black"
        android:text="+"
        android:textColor="#FF0000"
        android:textSize="60sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="297dp"
        android:layout_height="43dp"
        android:layout_marginTop="44dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="sans-serif-black"
        android:text="Vagas Abertas"
        android:textColor="#000000"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>